{
    "name": "mattrink/laravel-model-form-maker",
    "description": "A package to generate forms from model objects.",
    "homepage": "https://github.com/mattrink/laravel-model-form-maker",
    "keywords":
    [
        "laravel",
        "forms",
        "model"
    ],
    "authors": [
        {
            "name": "Matt Rink",
            "email": "matt.rink@gmail.com",
            "homepage": "https://mattrink.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "laravelcollective/html": "~5.5.0"

    },
    "require-dev": {
        "phpunit/phpunit": "~6.2"
    },
    "autoload": {
        "psr-4": {
            "MattRink\\LaravelModelFormMaker\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MattRink\\LaravelModelFormMaker\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "MIT",
